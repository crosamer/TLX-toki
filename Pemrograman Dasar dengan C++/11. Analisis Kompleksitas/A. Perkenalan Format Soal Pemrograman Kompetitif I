#include <bits/stdc++.h>
using namespace std;

#define ll long long
#define pb push_back

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int n;
    cin >> n;
    vector<int> temp;
    int hasil;

    for(int i = 0; i < n; i++){
        int x;
        cin >> x;
        temp.pb(x);
        hasil = hasil + temp[i];
    }

    for (int i = 0; i < n; i++){
        cout << hasil - temp[i] << endl;
    }

    return 0;
}
