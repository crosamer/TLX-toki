#include <bits/stdc++.h>
using namespace std;

int main(){
    
    int a, N;
    int terbesar = -9999999;
    int terkecil = 9999999;
    
    cin >> N;
    
    for (int i = 0; i < N; i++){
        cin >> a;
        
        if (a > terbesar){
            terbesar = a;
        }
        if (a < terkecil){
            terkecil = a;
        }
    }
    cout << terbesar << " " << terkecil;
    
    return 0;
}
